package DAO;

import java.util.Set;

import com.dawes.modelo.ingredientesVO;

public interface IngredientesDAO {
	int insertar(ingredientesVO ingrediente);
	int borrar(int idIngredientes);
	int borrar(String nombre);
	int modificar(ingredientesVO ingrediente);
	ingredientesVO leerIgualPorId(int idIngredientes);
	ingredientesVO leerIgualPorNombre(String nombre);
	Set< ingredientesVO> leerTodo();
}
