package DAO;

import java.util.Set;

import com.dawes.modelo.ingredientesVO;
import com.dawes.modelo.recetaVO;

public interface recetaDAO {
	int insertar(recetaVO receta);
	int borrar(int idReceta);
	int borrar(String nombreReceta);
	int modificar(recetaVO receta);
	ingredientesVO leerIgualPorId(int idIngredientes);
	ingredientesVO leerIgualPorNombre(int nombre);
	Set< ingredientesVO> leerTodo();
}
